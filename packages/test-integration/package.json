{
  "name": "@lvce-editor/search-process-test-integration",
  "version": "0.0.0-dev",
  "license": "MIT",
  "author": "Lvce Editor",
  "type": "module",
  "scripts": {
    "test-integration": "node --unhandled-rejections=warn --experimental-vm-modules ./node_modules/jest/bin/jest.js --detectOpenHandles --forceExit",
    "test:watch": "node --unhandled-rejections=warn --experimental-vm-modules ./node_modules/jest/bin/jest.js --watch"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.4",
    "@lvce-editor/rpc": "^4.13.0",
    "@types/ws": "^8.18.1",
    "electron": "^37.2.6",
    "execa": "^9.6.0",
    "get-port": "^7.1.0",
    "jest": "^30.0.4",
    "minimist": "^1.2.8",
    "ts-jest": "^29.4.1",
    "ws": "^8.18.3"
  },
  "engines": {
    "node": ">=22"
  },
  "jest": {
    "testTimeout": 15000,
    "injectGlobals": false,
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  }
}
