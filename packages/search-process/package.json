{
  "name": "@lvce-editor/search-process",
  "version": "0.0.0-dev",
  "description": "Search Process",
  "keywords": [
    "Lvce Editor",
    "Search"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lvce-editor/search-process.git"
  },
  "license": "MIT",
  "author": "Lvce Editor",
  "type": "module",
  "main": "src/searchProcessMain.ts",
  "bin": {
    "search-process": "bin/searchProcess.js"
  },
  "scripts": {
    "test": "node --unhandled-rejections=warn --experimental-vm-modules ./node_modules/jest/bin/jest.js --detectOpenHandles --forceExit",
    "test:watch": "node --unhandled-rejections=warn --experimental-vm-modules ./node_modules/jest/bin/jest.js --watch"
  },
  "jest": {
    "injectGlobals": false,
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 80,
        "lines": 80
      }
    }
  },
  "dependencies": {
    "@lvce-editor/assert": "^1.4.0",
    "@lvce-editor/constants": "^3.7.0",
    "@lvce-editor/rpc": "^4.20.0",
    "@lvce-editor/verror": "^1.7.0",
    "execa": "^9.6.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/node": "^24.0.15",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.5"
  },
  "optionalDependencies": {
    "@lvce-editor/ripgrep": "^4.0.0"
  },
  "engines": {
    "node": ">=24"
  }
}
